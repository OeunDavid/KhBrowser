<UserControl x:Class="ToolKHBrowser.Views.frmMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:dashboard="clr-namespace:ToolKHBrowser.Views.Controls"
             xmlns:numeric="http://gu.se/NumericInput"
             Height="auto"
             Width="auto"
             FontSize="11"
             Background="#06080D">
    <UserControl.Resources>
        <SolidColorBrush x:Key="DashBgBrush"
                         Color="#06080D"/>
        <SolidColorBrush x:Key="DashPanelBrush"
                         Color="#10131A"/>
        <SolidColorBrush x:Key="DashPanelBrush2"
                         Color="#0B0E14"/>
        <SolidColorBrush x:Key="DashBorderBrush"
                         Color="#232835"/>
        <SolidColorBrush x:Key="DashAccentBrush"
                         Color="#3B82F6"/>
        <SolidColorBrush x:Key="DashTextBrush"
                         Color="#E7ECF5"/>
        <SolidColorBrush x:Key="DashMutedBrush"
                         Color="#8C95A8"/>
        <SolidColorBrush x:Key="Menu.Static.Background"
                         Color="#141923"/>
        <SolidColorBrush x:Key="Menu.Static.Border"
                         Color="#232835"/>
        <SolidColorBrush x:Key="Menu.Static.Foreground"
                         Color="#FFFFFF"/>
        <SolidColorBrush x:Key="MenuItem.Highlight.Background"
                         Color="#1A2232"/>
        <SolidColorBrush x:Key="MenuItem.Highlight.Border"
                         Color="#2B3A57"/>
        <SolidColorBrush x:Key="MenuItem.Selected.Background"
                         Color="#1A2232"/>
        <SolidColorBrush x:Key="MenuItem.Selected.Border"
                         Color="#2B3A57"/>
        <SolidColorBrush x:Key="MenuItem.Pressed.Background"
                         Color="#1A2232"/>
        <SolidColorBrush x:Key="MenuItem.Pressed.Border"
                         Color="#2B3A57"/>
        <SolidColorBrush x:Key="MenuItem.Submenu.Background"
                         Color="#141923"/>
        <SolidColorBrush x:Key="MenuItem.Submenu.Border"
                         Color="#232835"/>
        <SolidColorBrush x:Key="MenuItem.Static.Background"
                         Color="#141923"/>
        <SolidColorBrush x:Key="MenuItem.Static.Border"
                         Color="Transparent"/>
        <SolidColorBrush x:Key="MenuItem.Static.Foreground"
                         Color="#FFFFFF"/>
        <SolidColorBrush x:Key="MenuItem.Disabled.Foreground"
                         Color="#7B8599"/>
        <SolidColorBrush x:Key="{x:Static SystemColors.MenuBrushKey}"
                         Color="#141923"/>
        <SolidColorBrush x:Key="{x:Static SystemColors.MenuTextBrushKey}"
                         Color="#FFFFFF"/>
        <SolidColorBrush x:Key="{x:Static SystemColors.GrayTextBrushKey}"
                         Color="#7B8599"/>

        <Style x:Key="DashboardCardBorderStyle"
               TargetType="Border">
            <Setter Property="Background"
                    Value="{StaticResource DashPanelBrush}"/>
            <Setter Property="BorderBrush"
                    Value="{StaticResource DashBorderBrush}"/>
            <Setter Property="BorderThickness"
                    Value="1"/>
            <Setter Property="CornerRadius"
                    Value="14"/>
            <Setter Property="Padding"
                    Value="10"/>
            <Setter Property="SnapsToDevicePixels"
                    Value="True"/>
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="14"
                                      ShadowDepth="0"
                                      Color="#66000000"
                                      Opacity="0.35"/>
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="{x:Type TextBox}">
            <Setter Property="Foreground"
                    Value="{StaticResource DashTextBrush}"/>
            <Setter Property="Background"
                    Value="#171B24"/>
            <Setter Property="BorderBrush"
                    Value="#2C3342"/>
            <Setter Property="BorderThickness"
                    Value="1"/>
            <Setter Property="Padding"
                    Value="8,4"/>
        </Style>

        <Style TargetType="{x:Type ComboBox}">
            <Setter Property="Foreground"
                    Value="{StaticResource DashTextBrush}"/>
            <Setter Property="Background"
                    Value="#171B24"/>
            <Setter Property="BorderBrush"
                    Value="#2C3342"/>
            <Setter Property="BorderThickness"
                    Value="1"/>
            <Setter Property="Padding"
                    Value="6,2"/>
        </Style>

        <Style TargetType="{x:Type Hyperlink}">
            <Setter Property="Foreground"
                    Value="{StaticResource DashAccentBrush}"/>
            <Setter Property="TextDecorations"
                    Value="{x:Null}"/>
        </Style>

        <Style TargetType="{x:Type DataGrid}">
            <Setter Property="Background"
                    Value="#0E1118"/>
            <Setter Property="Foreground"
                    Value="{StaticResource DashTextBrush}"/>
            <Setter Property="BorderBrush"
                    Value="{StaticResource DashBorderBrush}"/>
            <Setter Property="BorderThickness"
                    Value="0"/>
            <Setter Property="GridLinesVisibility"
                    Value="Horizontal"/>
            <Setter Property="HorizontalGridLinesBrush"
                    Value="#1B2130"/>
            <Setter Property="VerticalGridLinesBrush"
                    Value="#10131A"/>
            <Setter Property="RowBackground"
                    Value="#0E1118"/>
            <Setter Property="AlternatingRowBackground"
                    Value="#101521"/>
            <Setter Property="HeadersVisibility"
                    Value="Column"/>
        </Style>

        <Style TargetType="{x:Type DataGridColumnHeader}">
            <Setter Property="Background"
                    Value="#111622"/>
            <Setter Property="Foreground"
                    Value="{StaticResource DashMutedBrush}"/>
            <Setter Property="BorderBrush"
                    Value="#1E2433"/>
            <Setter Property="BorderThickness"
                    Value="0,0,0,1"/>
            <Setter Property="Padding"
                    Value="10,8"/>
            <Setter Property="FontWeight"
                    Value="SemiBold"/>
            <Setter Property="FontSize"
                    Value="11"/>
        </Style>

        <Style TargetType="{x:Type DataGridRow}">
            <Setter Property="Foreground"
                    Value="{StaticResource DashTextBrush}"/>
            <Setter Property="Background"
                    Value="#0E1118"/>
            <Setter Property="BorderBrush"
                    Value="#1B2130"/>
            <Setter Property="BorderThickness"
                    Value="0"/>
            <Style.Triggers>
                <Trigger Property="IsMouseOver"
                         Value="True">
                    <Setter Property="Background"
                            Value="#141A28"/>
                </Trigger>
                <Trigger Property="IsSelected"
                         Value="True">
                    <Setter Property="Background"
                            Value="#172033"/>
                    <Setter Property="Foreground"
                            Value="White"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style TargetType="{x:Type DataGridCell}">
            <Setter Property="BorderThickness"
                    Value="0"/>
            <Setter Property="Padding"
                    Value="8,3"/>
            <Setter Property="Background"
                    Value="Transparent"/>
            <Style.Triggers>
                <Trigger Property="IsSelected"
                         Value="True">
                    <Setter Property="Background"
                            Value="Transparent"/>
                    <Setter Property="Foreground"
                            Value="White"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style TargetType="{x:Type ContextMenu}">
            <Setter Property="Background"
                    Value="#141923"/>
            <Setter Property="Foreground"
                    Value="{StaticResource DashTextBrush}"/>
            <Setter Property="BorderBrush"
                    Value="{StaticResource DashBorderBrush}"/>
            <Setter Property="BorderThickness"
                    Value="1"/>
        </Style>

        <Style TargetType="{x:Type MenuItem}">
            <Setter Property="Foreground"
                    Value="White"/>
            <Setter Property="Background"
                    Value="Transparent"/>
            <Setter Property="BorderBrush"
                    Value="Transparent"/>
        </Style>
    </UserControl.Resources>
    <Grid Background="{StaticResource DashBgBrush}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="104"/>
            <RowDefinition Height="420"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="44"/>
        </Grid.RowDefinitions>
        <Border Grid.Column="0"
                Grid.Row="0"
                Grid.RowSpan="4"
                Background="#0A0D13"
                BorderBrush="#1B2130"
                BorderThickness="0,0,1,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="100"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="120"/>
                </Grid.RowDefinitions>

                <Border Grid.Row="0"
                        Margin="14,14,14,10"
                        Background="#0D1119"
                        BorderBrush="#1E2433"
                        BorderThickness="1"
                        CornerRadius="12"
                        Padding="12">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="42"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Border Width="36"
                                Height="36"
                                CornerRadius="10"
                                Background="#2563EB"
                                VerticalAlignment="Top">
                            <materialDesign:PackIcon Kind="CogOutline"
                                                     Width="20"
                                                     Height="20"
                                                     Foreground="White"
                                                     VerticalAlignment="Center"
                                                     HorizontalAlignment="Center"/>
                        </Border>
                        <StackPanel Grid.Column="1"
                                    Margin="10,0,0,0">
                            <TextBlock Text="NEXUS FB"
                                       Foreground="White"
                                       FontWeight="Bold"
                                       FontSize="16"/>
                            <TextBlock Text="V8.4 ENTERPRISE"
                                       Foreground="{StaticResource DashMutedBrush}"
                                       FontSize="10"
                                       Margin="0,2,0,0"/>
                        </StackPanel>
                    </Grid>
                </Border>

                <Grid Grid.Row="1"
                      Margin="14,6,14,10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <StackPanel Grid.Row="0">
                        <StackPanel.Resources>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="HorizontalContentAlignment"
                                        Value="Stretch"/>
                                <Setter Property="VerticalContentAlignment"
                                        Value="Center"/>
                                <Setter Property="Padding"
                                        Value="0"/>
                                <Setter Property="Foreground"
                                        Value="#F1F5F9"/>
                                <Setter Property="FontWeight"
                                        Value="SemiBold"/>
                                <Setter Property="BorderThickness"
                                        Value="0"/>
                                <EventSetter Event="MouseEnter"
                                             Handler="SidebarNavButton_MouseEnter"/>
                                <EventSetter Event="MouseLeave"
                                             Handler="SidebarNavButton_MouseLeave"/>
                            </Style>
                        </StackPanel.Resources>
                        <TextBlock Text="AUTOMATION"
                                   Foreground="{StaticResource DashMutedBrush}"
                                   FontSize="11"
                                   FontWeight="SemiBold"
                                   Margin="10,10,0,10"/>

                    <Button x:Name="btnNavShareConfig"
                            Tag="share"
                            Height="44"
                            Margin="0,0,0,8"
                            Background="#0F1F3D"
                            BorderBrush="#17346B"
                            BorderThickness="1"
                            FontWeight="SemiBold"
                            Click="SidebarConfigNav_Click">
                        <DockPanel LastChildFill="True">
                            <materialDesign:PackIcon Kind="ShareVariantOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Share Config"
                                       VerticalAlignment="Center"/>
                            <Ellipse Width="6"
                                     Height="6"
                                     Fill="#3B82F6"
                                     HorizontalAlignment="Right"
                                     Margin="8,0,10,0"/>
                        </DockPanel>
                    </Button>

                    <Button x:Name="btnNavGroupConfig"
                            Tag="groups"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="AccountGroupOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Groups Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavPageConfig"
                            Tag="pages"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="FileDocumentOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Pages Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavNewsFeedConfig"
                            Tag="newsfeed"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="NewspaperVariantOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="News Feed"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavProfileConfig"
                            Tag="profile"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="AccountCircleOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Profile Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavFriendsConfig"
                            Tag="friends"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="AccountMultiplePlusOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Friends Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavContactConfig"
                            Tag="contact"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="PhoneOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Contact Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavOtherConfig"
                            Tag="other"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="TuneVariant"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Other Config"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    <Button x:Name="btnNavCleanPcConfig"
                            Tag="cleanpc"
                            Height="38"
                            Margin="0,0,0,6"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            Click="SidebarConfigNav_Click">
                        <DockPanel>
                            <materialDesign:PackIcon Kind="TrashCanOutline"
                                                     Width="16"
                                                     Height="16"
                                                     Margin="10,0,8,0"
                                                     VerticalAlignment="Center"/>
                            <TextBlock Text="Clean PC"
                                       VerticalAlignment="Center"/>
                        </DockPanel>
                    </Button>
                    </StackPanel>

                    <Grid Grid.Row="2"
                          Margin="0,10,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="38"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Button Grid.Row="0"
                                Grid.Column="0"
                                Margin="0,0,4,0"
                                Height="32"
                                Background="#1D4ED8"
                                BorderBrush="#2563EB"
                                Foreground="White"
                                FontWeight="SemiBold"
                                FontSize="10"
                                Click="btnStore_Click">
                            <StackPanel Orientation="Horizontal"
                                        HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="Store"
                                                         Width="11"
                                                         Height="11"
                                                         Margin="0,0,4,0"/>
                                <TextBlock Text="Store"
                                           VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                        <Button Grid.Row="0"
                                Grid.Column="1"
                                Margin="4,0,0,0"
                                Height="32"
                                Background="#60A5FA"
                                BorderBrush="#60A5FA"
                                Foreground="White"
                                FontWeight="SemiBold"
                                FontSize="10"
                                Click="btnSettings_Click">
                            <StackPanel Orientation="Horizontal"
                                        HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="Cog"
                                                         Width="11"
                                                         Height="11"
                                                         Margin="0,0,4,0"/>
                                <TextBlock Text="Settings"
                                           VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                        <Button Grid.Row="1"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="0,6,0,0"
                                Height="32"
                                Background="#334155"
                                BorderBrush="#475569"
                                Foreground="#FB923C"
                                FontWeight="SemiBold"
                                FontSize="10"
                                Click="btnClearAllCache_Click">
                            <StackPanel Orientation="Horizontal"
                                        HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="Broom"
                                                         Width="11"
                                                         Height="11"
                                                         Margin="0,0,4,0"/>
                                <TextBlock Text="Clear Cache"
                                           VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>

                    </Grid>
                </Grid>

                <Grid Grid.Row="2"
                      Margin="14,6,14,14">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="36"/>
                        <RowDefinition Height="54"/>
                        <RowDefinition Height="42"/>
                    </Grid.RowDefinitions>
                    <DockPanel Grid.Row="0"
                               Margin="4,0,4,8">
                        <Ellipse Width="10"
                                 Height="10"
                                 Fill="#6B7280"
                                 Margin="0,0,8,0"
                                 VerticalAlignment="Center"/>
                        <TextBlock Text="System Idle"
                                   Foreground="{StaticResource DashMutedBrush}"
                                   VerticalAlignment="Center"/>
                        <TextBlock x:Name="txtSidebarElapsedTime"
                                   Text="00:00:00"
                                   Foreground="{StaticResource DashMutedBrush}"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Right"/>
                    </DockPanel>
                    <Button Grid.Row="1"
                            x:Name="btnSidebarStart"
                            Height="54"
                            FocusVisualStyle="{x:Null}"
                            Focusable="False"
                            Background="#2563EB"
                            BorderBrush="#2563EB"
                            Foreground="White"
                            FontWeight="Bold"
                            FontSize="14"
                            Click="btnSidebarStart_Click">
                        <StackPanel Orientation="Horizontal"
                                    HorizontalAlignment="Center">
                            <materialDesign:PackIcon x:Name="iconSidebarStart"
                                                     Kind="Play"
                                                     Width="18"
                                                     Height="18"
                                                     Margin="0,0,8,0"/>
                            <TextBlock x:Name="txtSidebarStartLabel"
                                       Text="START ENGINE"
                                       VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                    <Button Grid.Row="2"
                            x:Name="btnSidebarStop"
                            Margin="0,8,0,0"
                            Height="34"
                            FocusVisualStyle="{x:Null}"
                            Focusable="False"
                            Background="#1F2937"
                            BorderBrush="#374151"
                            Foreground="#FCA5A5"
                            FontWeight="SemiBold"
                            Click="btnStop_Click">
                        <StackPanel Orientation="Horizontal"
                                    HorizontalAlignment="Center">
                            <materialDesign:PackIcon Kind="Stop"
                                                     Width="14"
                                                     Height="14"
                                                     Margin="0,0,6,0"/>
                            <TextBlock Text="STOP"
                                       VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Grid>
            </Grid>
        </Border>

        <Border Grid.Column="1"
                Grid.Row="0"
                Margin="10,10,10,6"
                Style="{StaticResource DashboardCardBorderStyle}"
                Background="#0B0E14"
                Padding="12,10">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="140"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="140"/>
                    <ColumnDefinition Width="12"/>
                    <ColumnDefinition Width="170"/>
                    <ColumnDefinition Width="140"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="320"/>
                    <ColumnDefinition Width="8"/>
                    <ColumnDefinition Width="150"/>
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>

                <TextBlock Grid.Row="0"
                           Grid.Column="0"
                           Text="THREADS"
                           Foreground="{StaticResource DashMutedBrush}"
                           FontWeight="SemiBold"
                           VerticalAlignment="Center"
                           Margin="2,0,8,0"/>
                <numeric:SpinnerDecorator Grid.Row="0"
                                          Grid.Column="1"
                                          Margin="0,0,10,0">
                    <numeric:IntBox Name="txtThreadTopBar"
                                    AllowSpinners="True"
                                    Value="{Binding ElementName=txtThread, Path=Value, Mode=TwoWay}"
                                    Foreground="White"
                                    Background="#171B24"
                                    BorderBrush="#2C3342"
                                    BorderThickness="1"
                                    MinValue="1"
                                    MaxValue="1000000"
                                    PreviewTextInput="NumberValidationTextBox"/>
                </numeric:SpinnerDecorator>

                <TextBlock Grid.Row="0"
                           Grid.Column="2"
                           Text="RESET IP"
                           Foreground="{StaticResource DashMutedBrush}"
                           FontWeight="SemiBold"
                           VerticalAlignment="Center"
                           Margin="4,0,8,0"/>
                <numeric:SpinnerDecorator Grid.Row="0"
                                          Grid.Column="3"
                                          Margin="0,0,8,0">
                    <numeric:IntBox Name="txtResetIPTopBar"
                                    AllowSpinners="True"
                                    Value="{Binding ElementName=txtResetIP, Path=Value, Mode=TwoWay}"
                                    Foreground="White"
                                    Background="#171B24"
                                    BorderBrush="#2C3342"
                                    BorderThickness="1"
                                    MinValue="0"
                                    MaxValue="1000000"
                                    PreviewTextInput="NumberValidationTextBox"/>
                </numeric:SpinnerDecorator>

                <ComboBox Grid.Row="0"
                          Grid.Column="5"
                          Height="30"
                          Margin="0,0,8,0"
                          Style="{DynamicResource MaterialDesignComboBox}"
                          Background="#171B24"
                          BorderBrush="#2C3342"
                          Foreground="{StaticResource DashTextBrush}"
                          FocusVisualStyle="{x:Null}"
                          ItemsSource="{Binding ElementName=ddlStore, Path=ItemsSource}"
                          SelectedValue="{Binding ElementName=ddlStore, Path=SelectedValue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                          SelectedValuePath="Id"
                          DisplayMemberPath="Name"/>

                <ComboBox Grid.Row="0"
                          Grid.Column="6"
                          Height="30"
                          Margin="0,0,8,0"
                          Style="{DynamicResource MaterialDesignComboBox}"
                          Background="#171B24"
                          BorderBrush="#2C3342"
                          Foreground="{StaticResource DashTextBrush}"
                          FocusVisualStyle="{x:Null}"
                          ItemsSource="{Binding ElementName=ddlStatus, Path=ItemsSource}"
                          SelectedValue="{Binding ElementName=ddlStatus, Path=SelectedValue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                          SelectedValuePath="Id"
                          DisplayMemberPath="Name"/>

                <Border Grid.Row="0"
                        Grid.Column="8"
                        Height="38"
                        CornerRadius="19"
                        Background="#171B24"
                        BorderBrush="#2C3342"
                        BorderThickness="1"
                        VerticalAlignment="Center">
                    <DockPanel Margin="10,0">
                        <materialDesign:PackIcon Kind="Magnify"
                                                 Width="16"
                                                 Height="16"
                                                 Foreground="{StaticResource DashMutedBrush}"
                                                 Margin="0,0,8,0"
                                                 VerticalAlignment="Center"/>
                        <TextBox Name="txtKeywordTopBar"
                                 BorderThickness="0"
                                 Background="Transparent"
                                 Foreground="{StaticResource DashTextBrush}"
                                 Text="{Binding ElementName=txtKeyword, Path=Text, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                 KeyUp="txtKeyword_KeyUp"
                                 VerticalContentAlignment="Center"/>
                    </DockPanel>
                </Border>

                <Button Grid.Row="0"
                        Grid.Column="10"
                        Height="38"
                        MinWidth="150"
                        Background="#1D4ED8"
                        BorderBrush="#2563EB"
                        Foreground="White"
                        FontWeight="SemiBold"
                        Click="btnAddNewAccount_Click">
                    <StackPanel Orientation="Horizontal"
                                HorizontalAlignment="Center">
                        <materialDesign:PackIcon Kind="Plus"
                                                 Width="16"
                                                 Height="16"
                                                 Margin="0,0,6,0"/>
                        <TextBlock Text="New Account"
                                   VerticalAlignment="Center"/>
                    </StackPanel>
                </Button>

            </Grid>
        </Border>

        <Border Grid.Column="1"
                Grid.Row="1"
